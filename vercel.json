{
    "rewrites": [
      { "source": "/", "destination": "/pages/index.html" }
    ],
    "static": {
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=0, must-revalidate"
            }
          ]
        }
      ]
    }
  }
  